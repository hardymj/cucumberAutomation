{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "valid-jsdoc": "off",
        "require-jsdoc": "off",
        "guard-for-in": "off",
        "max-len": ["error", { "code": 120 }],
        "indent": ["error", 4],
        "no-tabs": ["error", { "allowIndentationTabs": true }]
    }
}
